<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <h2>Añadir paciente</h2>
            <small class="text-muted">Bienvenido a Odontología Especializada</small>
        </div>
        <div class="row clearfix">
			<div class="col-lg-12 col-md-12 col-sm-12">
				<form action="anadir_paciente" method="POST">
					<div class="body">
                        <div class="row clearfix">
                            <div class="col-lg-4 col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" name="nombre" placeholder="Nombres"  required> 
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-4 col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" name="apellido" placeholder="Apellidos"  required> 
                                    </div>
                                </div>
                            </div>

                            <div class="col-sm-3">
                                <div class="form-group">
                                  <div class="form-line">
                                    <input type="text" class="form-control" name="ci" placeholder="C.I."  required> 
                                  </div>
                                </div>
                            </div>                            

                            <div class="col-lg-4 col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" name="telefono" placeholder="Teléfono"  required>
                                    </div>
                                </div>
                            </div>


                            <div class="col-lg-3 col-md-6 col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input  type="date" class="datepicker form-control" name="fecha_nacimiento" placeholder="Fecha de nacimiento" required> 
                                    </div>
                                </div>
                            </div>

                            <div class="col-lg-3 col-md-6 col-sm-12">
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" name="correo_paciente"  placeholder="Correo electrónico"  required> 
                                    </div>
                                </div>
                               
                            </div>
                        </div>
                        <div class="row clearfix">                            
                            
                            <div class="col-lg-3 col-md-6 col-sm-12">
                            
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control"  name="enfermedades_base" placeholder="Enfermedades de base"  required>
                                    </div>
                                </div>
                            </div>
                            
                            <div class="col-lg-5 col-md-6 col-sm-12">
                                
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" name="alergias" placeholder="Alergias"  required> 
                                    </div>
                                </div>
                            </div>                            
                            

                            <div class="col-lg-4 col-md-6 col-sm-12">
                             
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" name="usuario" placeholder="Usuario"  required> 
                                    </div>
                                </div>
                            </div>


                            <div class="col-lg-6 col-md-6 col-sm-12">
                              
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" name="password" placeholder="Contraseña"  required> 
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-12">
                               
                                <div class="form-group">
                                    <div class="form-line">
                                        <input type="text" class="form-control" name="conf_password" placeholder="Confirmar contraseña"  required> 
                                    </div>
                                </div> 
                            </div> 
                          



                        </div>


                        <button type="submit" class="btn btn-raised g-bg-cyan">Enviar</button>
                        <a href="anadir-paciente" class="btn btn-raised">
                            Cancelar
                        </a>
                        


                           
                    </div>
                </form>

                
				</div>
			</div>
		</div> 
    </div>
